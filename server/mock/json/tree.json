        {
            "usage": "scaffold",
            "help": "Manipulate scaffold objects",
            "parent": "root"
        },
        {
            "usage": "generate",
            "help": "Generates a scaffold by some template",
            "parent": "root_scaffold"
        },
        {
            "usage": "coffee-go",
            "help": "Generates a project by coffee template in Go",
